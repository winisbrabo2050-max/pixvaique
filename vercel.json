{
  "version": 2,
  "builds": [
    { "src": "api/gerar-pix.js", "use": "@vercel/node" },
    { "src": "api/consultPix.js", "use": "@vercel/node" },
    { "src": "api/webhook.js", "use": "@vercel/node" },
    { "src": "api/status.js", "use": "@vercel/node" },
    { "src": "api/test-db.js", "use": "@vercel/node" },
    { "src": "api/get-transacoes.js", "use": "@vercel/node" },
    { "src": "api/auth.js", "use": "@vercel/node" },
    { "src": "api/upload.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/gerar-pix", "dest": "api/gerar-pix.js" },
    { "src": "/api/consultPix", "dest": "api/consultPix.js" },
    { "src": "/api/webhook", "dest": "api/webhook.js" },
    { "src": "/api/status", "dest": "api/status.js" },
    { "src": "/api/test-db", "dest": "api/test-db.js" },
    { "src": "/api/get-transacoes", "dest": "api/get-transacoes.js" },
    { "src": "/api/auth", "dest": "api/auth.js" },
    { "src": "/api/upload", "dest": "api/upload.js" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://admin-thofer.wuaze.com" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,POST,PUT,DELETE" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
      ]
    }
  ]
}
